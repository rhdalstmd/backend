    <script>
        // ë©”ì¸ ì´ë¯¸ì§€ ë³€ê²½
        function changeMainImage(thumbnail, imageSrc, imageNumber) {
            document.getElementById('mainImage').src = imageSrc;
            document.querySelector('.image-counter').textContent = `${imageNumber} / 5`;
            
            // ì¸ë„¤ì¼ í™œì„±í™” ìƒíƒœ ë³€ê²½
            document.querySelectorAll('.thumbnail').forEach(thumb => thumb.classList.remove('active'));
            thumbnail.classList.add('active');
        }
        
        // ìƒí’ˆ ìˆ˜ì •
        function editProduct() {
            alert('ìƒí’ˆ ìˆ˜ì • í˜ì´ì§€ë¡œ ì´ë™í•©ë‹ˆë‹¤.');
        }
        
        // ìƒíƒœ ë³€ê²½ ëª¨ë‹¬ í‘œì‹œ
        function showStatusModal() {
            document.getElementById('statusModal').classList.add('show');
        }
        
        // ìƒíƒœ ë³€ê²½ ëª¨ë‹¬ ë‹«ê¸°
        function closeStatusModal() {
            document.getElementById('statusModal').classList.remove('show');
        }
        
        // ìƒíƒœ ì—…ë°ì´íŠ¸
        function updateStatus() {
            const newStatus = document.getElementById('statusSelect').value;
            const statusBadge = document.getElementById('statusBadge');
            
            // ìƒíƒœ í…ìŠ¤íŠ¸ ë° í´ë˜ìŠ¤ ë³€ê²½
            statusBadge.className = 'status-badge';
            switch(newStatus) {
                case 'selling':
                    statusBadge.textContent = 'íŒë§¤ì¤‘';
                    statusBadge.classList.add('status-selling');
                    break;
                case 'reserved':
                    statusBadge.textContent = 'ì˜ˆì•½ì¤‘';
                    statusBadge.classList.add('status-reserved');
                    break;
                case 'sold':
                    statusBadge.textContent = 'íŒë§¤ì™„ë£Œ';
                    statusBadge.classList.add('status-sold');
                    break;
            }
            
            closeStatusModal();
            alert('ìƒí’ˆ ìƒíƒœê°€ ë³€ê²½ë˜ì—ˆìŠµë‹ˆë‹¤.');
        }
        
        // ìƒí’ˆ ì‚­ì œ
        function deleteProduct() {
            if (confirm('ì •ë§ë¡œ ì´ ìƒí’ˆì„ ì‚­ì œí•˜ì‹œê² ìŠµë‹ˆê¹Œ?\nì‚­ì œëœ ìƒí’ˆì€ ë³µêµ¬í•  ìˆ˜ ì—†ìŠµë‹ˆë‹¤.')) {
                alert('ìƒí’ˆì´ ì‚­ì œë˜ì—ˆìŠµë‹ˆë‹¤.');
            }
        }
        
        // ëŒì–´ì˜¬ë¦¬ê¸°
        function boostProduct() {
