                // í´ë¦½ë³´ë“œì— ë³µì‚¬
                navigator.clipboard.writeText(window.location.href);
                alert('ìƒí’ˆ ë§í¬ê°€ í´ë¦½ë³´ë“œì— ë³µì‚¬ë˜ì—ˆìŠµë‹ˆë‹¤.');
            }
